        "babel-loader": "^8.0.5",
        "babel-preset-vue": "^2.0.2",
        "eslint": "^5.13.0",
        "eslint-plugin-vue": "^4.7.0",
        "replace-in-file": "^3.4.3",
        "rollup": "^0.68.2",
        "rollup-plugin-babel": "^4.3.2",
        "rollup-plugin-replace": "^2.0.0",
        "rollup-plugin-vue": "^3.0.0",
        "uglify-js": "^3.4.9",
